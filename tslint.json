{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/"
        ]
    },
    "rules": {
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "jsx-boolean-value": false,
        "no-angle-bracket-type-assertion": false
    }
}